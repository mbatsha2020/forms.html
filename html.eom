    function getCookieEID(name) {
        var pair = document.cookie.match(new RegExp(name + '=([^;]+)'));
        return !!pair ? unescape(pair[1]) : '';
    };

    function setCookieEID(name, value, expires) {
        document.cookie = name + "=" + value + "; expires=" + expires;
    }

    (function () {
        var ecosystem = 'https://ecosystem.24.com'
        console.log("Checking for ecosystem cookie");
        if (getCookieEID('EcosystemId') != '') {
            console.log("Found ecosystem cookie");
            return;
        }
        window.addEventListener('message', function (event) {
            if (event.origin != ecosystem) return;
            console.log("Got ecosystem id");
            try {
                if (event.origin != ecosystem)
                    return;
                client_id = event.data;
                reg = RegExp(/^[0-9]{1,}\.[0-9]{1,}$/)
                if (client_id.match(reg) === null) {
                    return;
                }
                dte = new Date()
                dte.setDate(dte.getDate() + 730)
                setCookieEID("EcosystemId", client_id, dte.toUTCString())
            } catch (err) {
                console.log("Error writing ecosystem cookie: " + err)
            }
        });
